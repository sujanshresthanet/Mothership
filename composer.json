{
    "name": "stwt/mothership",
    "description": "A Laravel based CRUD app framework.",
    "authors": [
        {
            "name": "Jim Wardlaw",
            "email": "jim@stwt.co"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "cartalyst/sentry": "2.0.*",
        "laravelbook/ardent": "dev-master",
        "venturecraft/revisionable": "1.*",
        "jasonlewis/expressive-date": "1.0.*",
        "cviebrock/eloquent-sluggable": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/lang",
            "src/controllers",
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Stwt\\Mothership": "src/"
        }
    },
    "minimum-stability": "dev"
}